<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:s="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="urn:wsdl" targetNamespace="urn:wsdl" xmlns:ns="http://schemas.intersystems.ru/hs/ehr/v1">
<wsdl:types>
<s:schema xmlns:tns="http://schemas.intersystems.ru/hs/ehr/v1" xmlns:__tns="http://schemas.intersystems.ru/hs/ehr/v1" targetNamespace="http://schemas.intersystems.ru/hs/ehr/v1" elementFormDefault="qualified">
  <s:complexType name="Container">
    <s:annotation>
      <s:documentation>Пакет данных, поступивший от МИС</s:documentation>
    </s:annotation>
    <s:sequence>
      <s:element minOccurs="0" name="patient" type="tns:Patient">
        <s:annotation>
          <s:documentation>Пациент</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="sourceDocument" type="tns:ContainerDocument">
        <s:annotation>
          <s:documentation>Документ, в рамках которого поступили данные</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="encounters" type="tns:ArrayOfencounterEncounter">
        <s:annotation>
          <s:documentation>Эпизоды</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="diagnoses" type="tns:ArrayOfdiagnosisDiagnosis">
        <s:annotation>
          <s:documentation>Диагнозы</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="disabilities" type="tns:ArrayOfdisabilityDisability">
        <s:annotation>
          <s:documentation>Инвалидность</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="sickLeaveDocuments" type="tns:ArrayOfsickLeaveDocumentSickLeaveDocument">
        <s:annotation>
          <s:documentation>Документы временной нетрудоспособности</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="allergies" type="tns:ArrayOfallergyAllergy">
        <s:annotation>
          <s:documentation>Аллергические реакции и лекарственная непереносимость</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="documents" type="tns:ArrayOfdocumentDocument">
        <s:annotation>
          <s:documentation>Документы (в том числе эпикризы, дневниковые записи, ...)</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="services" type="tns:ArrayOfserviceMedService">
        <s:annotation>
          <s:documentation>Мед. услуги</s:documentation>
        </s:annotation>
      </s:element>
    </s:sequence>
    <s:attribute name="facilityCode" type="tns:String" use="required">
      <s:annotation>
        <s:documentation>Код ЛПУ
ЕГИСЗ справочник 1.2.643.5.1.13.2.1.1.178 (MDR308)</s:documentation>
      </s:annotation>
    </s:attribute>
    <s:attribute name="patientMRN" type="tns:String" use="required">
      <s:annotation>
        <s:documentation>Идентификатор пациента в МИС ЛПУ</s:documentation>
      </s:annotation>
    </s:attribute>
  </s:complexType>
  <s:simpleType name="String">
    <s:restriction base="s:string">
      <s:minLength value="1"/>
    </s:restriction>
  </s:simpleType>
  <s:complexType name="Patient">
    <s:annotation>
      <s:documentation>Пациент</s:documentation>
    </s:annotation>
    <s:sequence>
      <s:element minOccurs="0" name="baseClinic" type="tns:CodeAndName">
        <s:annotation>
          <s:documentation>Поликлиника прикрепления
ТФОМС: LPUBASE - из регионального справочника</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="snils" type="tns:Snils">
        <s:annotation>
          <s:documentation>СНИЛС
ТФОМС: SNILS
ЕГИСЗ "Пациент" поле №3
СЭМД ext:asOtherIDs[classCode='IDENT']/ext:documentNumber/@number (ext:documentType code="3" codeSystem="1.2.643.5.1.13.2.7.1.62")</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="enp" type="tns:Enp">
        <s:annotation>
          <s:documentation>ЕНП
ТФОМС: ENP</s:documentation>
        </s:annotation>
      </s:element>
      <s:element name="familyName" type="tns:String">
        <s:annotation>
          <s:documentation>Фамилия
ТФОМС: FAM
ЕГИСЗ "Пациент" поле №5</s:documentation>
        </s:annotation>
      </s:element>
      <s:element name="givenName" type="tns:String">
        <s:annotation>
          <s:documentation>Имя
ТФОМС: IM
ЕГИСЗ "Пациент" поле №6</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="middleName" type="tns:String">
        <s:annotation>
          <s:documentation>Отчество
ТФОМС: OT
ЕГИСЗ "Пациент" поле №7</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="prevNames" type="tns:ArrayOfNameName">
        <s:annotation>
          <s:documentation>ФИО в прошлом</s:documentation>
        </s:annotation>
      </s:element>
      <s:element name="dob" type="s:date">
        <s:annotation>
          <s:documentation>Дата рождения
ТФОМС: DR
ЕГИСЗ "Пациент" поле №8</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="birthAddress" type="tns:Address">
        <s:annotation>
          <s:documentation>Место рождения
ТФОМС: MR (записывается в city)
ЕГИСЗ "Пациент" поле №9</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="gender">
        <s:annotation>
          <s:documentation>Пол
ТФОМС: W (V005: 1=M, 2=F)
deprecated: ЕГИСЗ codeSystem: 2.16.643.1.113883.5.1
ЕГИСЗ "Пациент" поле №11
СЭМД codeSystem: 1.2.643.5.1.13.2.1.1.156 /C51007  (1=M, 2=F, 3=Не определенный)</s:documentation>
        </s:annotation>
        <s:simpleType>
          <s:restriction base="s:string">
            <s:enumeration value="M"/>
            <s:enumeration value="F"/>
          </s:restriction>
        </s:simpleType>
      </s:element>
      <s:element minOccurs="0" name="legalAddress" type="tns:Address">
        <s:annotation>
          <s:documentation>Адрес прописки (постоянного места жительства)
ТФОМС: OKATOG
ЕГИСЗ "Пациент" таблица данных адреса и контактов, поля №2-11 (тип адреса = 1)</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="actualAddress" type="tns:Address">
        <s:annotation>
          <s:documentation>Адрес пребывания
ТФОМС: OKATOP
ЕГИСЗ "Пациент" таблица данных адреса и контактов, поля №2-11 (тип адреса = 2)</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="postalAddress" type="tns:Address">
        <s:annotation>
          <s:documentation>Адрес почтовый
ЕГИСЗ "Пациент" таблица данных адреса и контактов, поля №2-11 (тип адреса = 3)</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="prevAddresses" type="tns:ArrayOfaddressAddress">
        <s:annotation>
          <s:documentation>Предыдущие адреса
ф 025/у-04</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="dwellingType">
        <s:annotation>
          <s:documentation>Признак городского/сельского жителя
ф 025-12/у, 003/у
ЕГИСЗ Код: PRK438, OID: 1.2.643.5.1.13.2.1.1.573 (1=город, 2=село)
ЕГИСЗ "Пациент" таблица данных адреса и контактов, поле №6
СЭМД ext:asResidenceType codeSystem="1.2.643.5.1.13.2.1.1.573"</s:documentation>
        </s:annotation>
        <s:simpleType>
          <s:restriction base="s:string">
            <s:enumeration value="1"/>
            <s:enumeration value="2"/>
          </s:restriction>
        </s:simpleType>
      </s:element>
      <s:element maxOccurs="unbounded" minOccurs="0" name="privilege" nillable="true" type="tns:Privilege">
        <s:annotation>
          <s:documentation>Данные о льготах
ф 025/у-04, 025-12/у
СЭМД recordTarget/patientRole/patient/ext:patient/ext:code[@codeSystem='1.2.643.5.1.13.2.1.1.358']/@code</s:documentation>
        </s:annotation>
      </s:element>
      <s:element maxOccurs="unbounded" minOccurs="0" name="socialStatus" nillable="true" type="tns:CodeAndName">
        <s:annotation>
          <s:documentation>Социальный статус
ф 025-12/у
ЕГИСЗ Код: SMP468, OID: 1.2.643.5.1.13.2.1.1.366
СЭМД recordTarget/patientRole/patient/ext:patient/ext:code[@codeSystem='1.2.643.5.1.13.2.1.1.366']/@code</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="isHomeless" type="s:boolean">
        <s:annotation>
          <s:documentation>Признак лица без определенного места жительства
ф 025-12/у</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="isServicemanFamily" type="s:boolean">
        <s:annotation>
          <s:documentation>Признак "член семьи военнослужащего"
ф 025-12/у</s:documentation>
        </s:annotation>
      </s:element>
      <s:element maxOccurs="unbounded" minOccurs="0" name="occupation" nillable="true" type="tns:Occupation">
        <s:annotation>
          <s:documentation>Место работы/учебы, профессия, должность
ф 027у, 025/у-04, 003/у
ЕГИСЗ "Пациент" поля №№20-24</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="prevOccupations" type="tns:ArrayOfoccupationHistoryEntry">
        <s:annotation>
          <s:documentation>Предыдущие места работы/учебы/службы
ф 025/у-04</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="citizenship" type="tns:CodeAndName">
        <s:annotation>
          <s:documentation>Гражданство</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="identityDocument" type="tns:IdentityDocument">
        <s:annotation>
          <s:documentation>Документ, удостоверяющий личность
ТФОМС: DOCTYPE (F011), DOCSER, DOCNUM
ЕГИСЗ "Пациент" поля №№12-15
СЭМД ext:asOtherIDs classCode="IDENT" (ext:documentType code="5" codeSystem="1.2.643.5.1.13.2.7.1.62")</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="prevIdentityDocuments" type="tns:ArrayOfidentityDocumentIdentityDocument">
        <s:annotation>
          <s:documentation>Устаревшие документы, удостоверяющие личность</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="birthCertificate" type="tns:IdentityDocument">
        <s:annotation>
          <s:documentation>Свидетельство о рождении
В F011 тип документа = 3</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="omsInsurance" type="tns:Insurance">
        <s:annotation>
          <s:documentation>Данные полиса ОМС
ТФОМС: VPOLIS (F008), SPOLIS, NPOLIS
ЕГИСЗ "Пациент" поля №№26-30
СЭМД ext:asOtherIDs classCode="HLD" (ext:documentType code="1" codeSystem="1.2.643.5.1.13.2.7.1.62")</s:documentation>
        </s:annotation>
      </s:element>
      <s:element maxOccurs="unbounded" minOccurs="0" name="dmsInsurance" nillable="true" type="tns:Insurance">
        <s:annotation>
          <s:documentation>Данные полиса ДМС
ЕГИСЗ "Пациент" таблица сведений о ДМС
СЭМД ext:asOtherIDs classCode="HLD" (ext:documentType code="2" codeSystem="1.2.643.5.1.13.2.7.1.62")</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="bloodGroup">
        <s:annotation>
          <s:documentation>Группа крови</s:documentation>
        </s:annotation>
        <s:simpleType>
          <s:restriction base="s:string">
            <s:enumeration value="1"/>
            <s:enumeration value="2"/>
            <s:enumeration value="3"/>
            <s:enumeration value="4"/>
          </s:restriction>
        </s:simpleType>
      </s:element>
      <s:element minOccurs="0" name="rhesusFactor" type="s:boolean">
        <s:annotation>
          <s:documentation>Резус-фактор</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="contactInfo" type="tns:ContactInfo">
        <s:annotation>
          <s:documentation>Контактная информация
ЕГИСЗ "Пациент" таблица данных адреса и контактов, поля №№12-13</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="isTrustee" type="s:boolean">
        <s:annotation>
          <s:documentation>Признак того, что данные не пациента, а его представителя (данные пациента пустые)
ТФОМС: PDT (1=false/2=true)
НЕ ИСПОЛЬЗУЕТСЯ</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="trusteeList" type="tns:ArrayOftrusteeTrustee">
        <s:annotation>
          <s:documentation>Представители/опекуны/родители/контактные лица пациента
ЕГИСЗ "Представитель пациента"
СЭМД participant - Контактные лица пациента
СЭМД recordTarget/patientRole/patient/guardian - Опекун/родитель</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="newBornData" type="tns:String">
        <s:annotation>
          <s:documentation>Данные новорожденного
ТФОМС: NOVOR: ПДДММГГН, где П – пол ребёнка в соответствии с классификатором V005; ДД – день рождения; ММ – месяц рождения; ГГ – последние две цифры года рождения;Н – порядковый номер ребёнка</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="isUnidentified" type="s:boolean">
        <s:annotation>
          <s:documentation>Признак неидентифицированного
ЕГИСЗ "Пациент" поле №2
СЭМД ext:asUnidentified</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="isForeigner" type="s:boolean">
        <s:annotation>
          <s:documentation>Признак: иностранный гражданин без регистрации на территории РФ
ЕГИСЗ "Пациент" таблица данных адреса и контактов, поле №14</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="inn" type="tns:Inn">
        <s:annotation>
          <s:documentation>ИНН
ЕГИСЗ "Пациент" поле №4
СЭМД ext:asOtherIDs[classCode='IDENT']/ext:documentNumber/@number (ext:documentType code="4" codeSystem="1.2.643.5.1.13.2.7.1.62")</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="race" type="tns:CodeAndName">
        <s:annotation>
          <s:documentation>Рассовая принадлежность
ЕГИСЗ "Пациент" поле №16</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="maritalStatus" type="tns:CodeAndName">
        <s:annotation>
          <s:documentation>Семейное положение
ЕГИСЗ "Пациент" поле №17</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="children" type="s:long">
        <s:annotation>
          <s:documentation>Количество детей
ЕГИСЗ "Пациент" поле №18</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="levelOfEducation" type="tns:CodeAndName">
        <s:annotation>
          <s:documentation>Уровень образования
ЕГИСЗ "Пациент" поле №19</s:documentation>
        </s:annotation>
      </s:element>
    </s:sequence>
  </s:complexType>
  <s:complexType name="CodeAndName">
    <s:complexContent>
      <s:extension base="tns:BaseSerial">
        <s:sequence>
          <s:element minOccurs="0" name="code" type="tns:String">
            <s:annotation>
              <s:documentation>Код</s:documentation>
            </s:annotation>
          </s:element>
          <s:element minOccurs="0" name="name" type="tns:String">
            <s:annotation>
              <s:documentation>Расшифровка</s:documentation>
            </s:annotation>
          </s:element>
          <s:element minOccurs="0" name="codingSystem" type="tns:String">
            <s:annotation>
              <s:documentation>Система кодификации (используемый справочник)</s:documentation>
            </s:annotation>
          </s:element>
        </s:sequence>
      </s:extension>
    </s:complexContent>
  </s:complexType>
  <s:complexType name="BaseSerial"/>
  <s:complexType name="Employee">
    <s:sequence>
      <s:element minOccurs="0" name="code" type="tns:String">
        <s:annotation>
          <s:documentation>Код
ТФОМС: IDDOKT --&gt; CODE_MD - региональный справочник DOCTORS
СЭМД author/assignedAuthor/id/@extension, legalAuthenticator/assignedEntity/id/@extension, authenticator/assignedEntity/id/@extension</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="name" type="tns:String">
        <s:annotation>
          <s:documentation>Расшифровка (если известны ФИО сотрудника, то поле name не следует заполнять)</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="codingSystem" type="tns:String">
        <s:annotation>
          <s:documentation>Система кодификации</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="specialty" type="tns:CodeAndName">
        <s:annotation>
          <s:documentation>Специальность
ТФОМС: PRVS (V004 - Классификатор медицинских специальностей)
PnR (Секция описания автора документа) - authorSpecialty
СЭМД author/assignedAuthor/ext:asLicencedEntity/ext:code codeSystem="1.2.643.5.1.13.2.1.1.181"</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="role" type="tns:CodeAndName">
        <s:annotation>
          <s:documentation>Должность
PnR (Секция описания автора документа) - authorRole
СЭМД: author/assignedAuthor/code  Справочник MDP365, OID: 1.2.643.5.1.13.2.1.1.607</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="snils" type="tns:Snils">
        <s:annotation>
          <s:documentation>СНИЛС
PnR (Секция описания автора документа) - authorPerson, legalAuthenticator (пример: 00000000003^Привалов^Александр^Иванович)</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="familyName" type="tns:String">
        <s:annotation>
          <s:documentation>Фамилия
СЭМД author/assignedAuthor/assignedPerson/name/family, legalAuthenticator/assignedEntity/assignedPerson/name/family, authenticator/assignedEntity/assignedPerson/name/family</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="givenName" type="tns:String">
        <s:annotation>
          <s:documentation>Имя
СЭМД author/assignedAuthor/assignedPerson/name/given, legalAuthenticator/assignedEntity/assignedPerson/name/given, authenticator/assignedEntity/assignedPerson/name/given</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="middleName" type="tns:String">
        <s:annotation>
          <s:documentation>Отчество
СЭМД author/assignedAuthor/assignedPerson/name/given, legalAuthenticator/assignedEntity/assignedPerson/name/given, authenticator/assignedEntity/assignedPerson/name/given</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="contactInfo" type="tns:ContactInfo">
        <s:annotation>
          <s:documentation>Контактная информация
СЭМД author/assignedAuthor/telecom/@value, legalAuthenticator/assignedEntity/telecom/@value, authenticator/assignedEntity/telecom/@value</s:documentation>
        </s:annotation>
      </s:element>
      <s:element minOccurs="0" name="fromTime" type="s:dateTime">
        <s:annotation>
          <s:documentation>Дата начала деятельности
СЭМД author/time/@value, legalAuthenticator/time/@value, authenticator/time/@value</s:documentation>
        </s:annotation>
      </s:element>
    </s:sequence>
  </s:complexType>
  <s:complexType name="CDAFacility">
    <s:complexContent>
      <s:extension base="tns:CodeAndName">
        <s:sequence>
          <s:element minOccurs="0" name="facilityAddress" type="tns:Address">
            <s:annotation>
              <s:documentation>Адрес ЛПУ</s:documentation>
            </s:annotation>
          </s:element>
          <s:element minOccurs="0" name="contactInfo" type="tns:ContactInfo">
