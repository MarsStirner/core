<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <filter>
        <filter-name>gzipFilter</filter-name>
        <filter-class>
            net.sf.ehcache.constructs.web.filter.GzipFilter
        </filter-class>
    </filter>
    <filter-mapping>
        <filter-name>gzipFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>medipad</servlet-name>
        <servlet-class>
            ru.korus.tmis.ws.medipad.impl.MedipadWSImpl
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>authentication</servlet-name>
        <servlet-class>
            ru.korus.tmis.ws.medipad.impl.AuthenticationWSImpl
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
</web-app>
