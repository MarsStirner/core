        <mysql.db.jndi.mis>s11r64</mysql.db.jndi.mis>
        <mysql.db.jndi.rls>rls</mysql.db.jndi.rls>
        <mysql.db.jndi.tmis_core>tmis_core</mysql.db.jndi.tmis_core>
        <mysql.db.jndi.kladr>kladr</mysql.db.jndi.kladr>


