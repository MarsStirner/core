        <mysql.db.mis.pool>s11r64-pool</mysql.db.mis.pool>
        <!-- Название базы данных RLS -->
        <mysql.db.rls>rls</mysql.db.rls>
        <!-- Название пула для базы данных RLS -->
        <mysql.db.rls.pool>rls-pool</mysql.db.rls.pool>
